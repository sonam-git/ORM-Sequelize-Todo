

<hr>
{{#if loggedInUser}}
<div class="container">
 <h1 class="welcome">Welcome {{ capitalizeFirstLetter loggedInUser.username }}</h1>
  <button id="logout" class="logout-btn">Log Out</button>
</div>

  <ul class="userList">
    {{#each users}}
      {{> list_render_partials}}  
    {{/each}}
  </ul>
 {{else}}
  <h1>Sign Up</h1>
    <p>If you are new user please use Sign up form.</p>
   
    <a href="/login"> <button> ‚Üê Already Signed Up</button> </a>
     <br>
     <br>
  <!-- Sign-up form -->

  <form id="signupForm">
    <input id="signupUsername"  class="password"  type="text" name="username" placeholder=" signup - username"/>
    <input id="signupPassword"  class="password"  type="password" name="password" placeholder=" signup - password"/>
    <button id="signupBtn">Sign Up</button> 
  </form>
<hr>





{{/if}}
  

